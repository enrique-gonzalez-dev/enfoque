<% edit ||= "#" %>
<% id ||= 0 %>
<% title ||= "Detalles" %>

<div class="card-title mb-3">     
  <div class="row">
   <div class="panel-title-col col-xl-10 col-lg-10 col-md-10 col-sm-9">
     <%= title %>
   </div>

	<div class="panel-controls-col col-xl-2 col-lg-2 col-md-2 col-sm-3">
		<ul class="panel-controls-ul">
		<%= render "helpers/panel_edit", edit: edit %>
		<%= render "helpers/panel_delete", id: id %>
		</ul>
	</div>   
 </div>
</div>
